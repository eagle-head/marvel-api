    "@testing-library/user-event": "^12.8.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "scripts": {
    "svgr": "rm -rf src/assets/svg && svgr --typescript --expand-props 'end' --memo --out-dir src/assets/ assets/ && npm run format",
    "start": "cross-env webpack serve --config webpack/webpack.dev.js --color --progress",
    "build": "cross-env webpack --config webpack/webpack.prod.js --color --progress",
    "lint": "eslint --fix \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "test": "jest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@svgr/cli": "^5.5.0",
    "@svgr/webpack": "^5.5.0",
